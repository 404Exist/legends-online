{
  "name": "kokspflanze/pserver-cms",
  "homepage": "https://kokspflanze.github.io/PServerCMS/#/",
  "description": "CMS for pservers",
  "keywords": [
    "framework",
    "laminas",
    "twig"
  ],
  "autoload": {
    "psr-4": {
      "Customize\\": "module/Customize/src/Customize",
      "GameBackend\\": "module/GameBackend/src",
      "PServerAdmin\\": "module/PServerAdmin/src",
      "PServerAdminStatistic\\": "module/PServerAdminStatistic/src",
      "PServerCLI\\": "module/PServerCLI/src",
      "PServerCore\\": "module/PServerCore/src",
      "PServerCMS\\DonateAction\\": "module/PServerDonateAction/src",
      "PServerPanel\\": "module/PServerPanel/src",
      "PServerRanking\\": "module/PServerRanking/src",
      "PServerCMS\\RankingMain\\": "module/PServerRankingMain/src",
      "PServerSRO\\": "module/PServerSRO/src",
      "PServerCMS\\SROUnique\\": "module/PServerSROUnique/src",
      "PaymentAPI\\": "module/PaymentAPI/src",
      "PServerCMS\\SROItemPoints\\": "module/SROItemPoints/src",
      "PServerCMS\\SROKill\\": "module/PServerSROKill/src",
      "SROItemDetails\\": "module/SROItemDetails/src/SROItemDetails"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PaymentAPITest\\": "module/PaymentAPI/tests/PaymentAPITest",
      "PServerCoreTest\\": "module/PServerCore/tests/PServerCoreTest",
      "PServerCMSTest\\DonateAction\\": "module/PServerDonateAction/tests"
    }
  },
  "require": {
    "php": "^7.4",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-gettext": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "doctrine/doctrine-laminas-hydrator": "^2.0",
    "doctrine/doctrine-orm-module": "^4.1.0 || ^5.0.1",
    "erusev/parsedown": "^1.6.3",
    "guzzlehttp/guzzle": "^6.3",
    "kokspflanze/bjy-authorize": "dev-master as 2.3.0",
    "kokspflanze/pdo-dblib-module": "^2.0",
    "kokspflanze/small-user": "^3.1.1",
    "kokspflanze/zfc-bbcode": "^2.3",
    "kokspflanze/zfc-sitemap": "dev-multi-domain",
    "kokspflanze/zfc-ticket-system": "^2.2.0",
    "kokspflanze/zfc-twig": "^4.0.1",
    "laminas/laminas-cache": "^2.13.0 || ^3.1.2",
    "laminas/laminas-cache-storage-adapter-filesystem": "@stable",
    "laminas/laminas-cache-storage-adapter-memory": "@stable",
    "laminas/laminas-developer-tools": "^2.2.0",
    "laminas/laminas-escaper": "^2.9.0",
    "laminas/laminas-filter": "^2.12.0",
    "laminas/laminas-form": "^3.1.1",
    "laminas/laminas-http": "^2.15.1",
    "laminas/laminas-hydrator": "^4.3.1",
    "laminas/laminas-i18n": "^2.13.0",
    "laminas/laminas-i18n-resources": "^2.8.0",
    "laminas/laminas-inputfilter": "^2.12.1",
    "laminas/laminas-log": "^2.14",
    "laminas/laminas-mail": "^2.15.1",
    "laminas/laminas-mime": "^2.9.1",
    "laminas/laminas-modulemanager": "^2.11.0",
    "laminas/laminas-mvc": "^3.3.0",
    "laminas/laminas-mvc-console": "^1.3.0",
    "laminas/laminas-mvc-i18n": "^1.3.0",
    "laminas/laminas-mvc-plugin-flashmessenger": "^1.4.0",
    "laminas/laminas-mvc-plugin-identity": "^1.3.0",
    "laminas/laminas-navigation": "^2.11.1",
    "laminas/laminas-paginator": "^2.11.0",
    "laminas/laminas-recaptcha": "^3.4.0",
    "laminas/laminas-router": "^3.5.0",
    "laminas/laminas-serializer": "^2.11.0",
    "laminas/laminas-servicemanager": "^3.7.0",
    "laminas/laminas-session": "^2.12.0",
    "laminas/laminas-stdlib": "^3.6.1",
    "laminas/laminas-validator": "^2.14.1",
    "laminas/laminas-view": "^2.14.2",
    "san/san-captcha": "^2.0.1",
    "slm/locale": "~0.5",
    "stripe/stripe-php": "^10.3",
    "symfony/lock": "^4.2",
    "zfc-datagrid/zfc-datagrid": "dev-master"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "replace": {
    "laminas/laminas-cache-storage-adapter-apc": "*",
    "laminas/laminas-cache-storage-adapter-dba": "*",
    "laminas/laminas-cache-storage-adapter-ext-mongodb": "*",
    "laminas/laminas-cache-storage-adapter-memcache": "*",
    "laminas/laminas-cache-storage-adapter-mongodb": "*",
    "laminas/laminas-cache-storage-adapter-wincache": "*",
    "laminas/laminas-cache-storage-adapter-xcache": "*"
  }
}
