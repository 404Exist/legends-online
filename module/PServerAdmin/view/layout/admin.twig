{{ doctype() }}
<html lang="en">
<head>
	<meta charset="utf-8">
	<title>
        {{ navigationWidgetPServerCore().menu('Laminas\\Navigation\\PServerAdmin').getContainer().findAllBy('active', true)|last.getLabel }}{% block title %}{% endblock title %} >> {{ translate('PServerAdmin') }}
	</title>

	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	{% block meta %}{% endblock meta %}

    {{ headLink({'rel' : 'icon', 'type' : 'image/x-icon', 'href' : 'data:image/png;base64,'}, 'PREPEND')
		.appendStylesheet(
			'https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css',
			'screen',
			true,
			{
				'crossorigin' : 'anonymous',
				'integrity' : 'sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO'
			}
		)
		.appendStylesheet(
			'https://code.jquery.com/ui/1.12.1/themes/smoothness/jquery-ui.min.css',
			'screen',
			true,
			{'crossorigin' : 'anonymous'}
		)
		.appendStylesheet(
			'https://use.fontawesome.com/releases/v5.5.0/css/all.css',
			'screen',
			true,
			{
				'crossorigin' : 'anonymous',
                'integrity' : 'sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU'
			}
		)
		.appendStylesheet(
			basePath('/assets/admin/css/sb-admin-2.min.css'),
			'screen'
		)|raw
    }}

	<style>
		.sceditor-group *,.sceditor-group *:before,.sceditor-group *:after {
			-moz-box-sizing: content-box;
			-webkit-box-sizing: content-box;
			box-sizing: content-box;
		}
		.sceditor-container textarea {
			height: 150px;
		}
	</style>
	{% block style %}{% endblock style %}

</head>
<body id="page-top">
	<nav class="navbar navbar-expand navbar-light bg-light static-top">

		{% if isAllowed('PServerAdmin/home') %}
			<a class="navbar-brand mr-1" href="{{ url('PServerAdmin/home') }}">{{ translate('PServerAdmin') }}</a>
		{% endif %}

		<button class="btn btn-link btn-sm order-1 order-sm-0" id="sidebarToggle" href="#">
			<i class="fas fa-bars"></i>
		</button>

		<!-- Navbar Search -->
		<div class="d-none d-md-inline-block form-inline ml-auto mr-0 mr-md-3 my-2 my-md-0">

		</div>

		<!-- Navbar -->

		<ul class="navbar-nav ml-auto ml-md-0">
			<li class="nav-item no-arrow mx-1">
				<a class="nav-link" href="{{ url('PServerCore') }}"><i class="fas fa-desktop"></i></a>
			</li>
			{% if isAllowed('PServerAdmin/donate') %}
				<li class="nav-item no-arrow mx-1 d-none d-md-inline d-lg-inline d-xl-inline">
					<a class="nav-link" href="{{ url('PServerAdmin/donate') }}"><i class="fas fa-donate" title="{{ translate('Donations') }}"></i> <span class="badge badge-danger">{{ donateCounter() }}</span></a>
				</li>
				<li class="nav-item no-arrow mx-1 d-none d-md-inline d-lg-inline d-xl-inline">
					<a class="nav-link" href="{{ url('PServerAdmin/donate') }}"><i class="fas fa-dollar-sign" title="{{ translate('Total Donations') }}"></i> <span class="badge badge-danger">{{ donateSum() }}</span></a>
				</li>
			{% endif %}
			{% if isAllowed('zfc-ticketsystem-admin') %}
				<li class="nav-item no-arrow mx-1 d-none d-md-inline d-lg-inline d-xl-inline">
					<a class="nav-link" href="{{ url('zfc-ticketsystem-admin', {'type': '0'}) }}"><i class="fas fa-tasks" title="{{ translate('New Tickets') }}"></i> <span class="badge badge-danger">{{ numberOfNewTickets() }}</span></a>
				</li>
			{% endif %}
			{% if isAllowed('small-user-auth','logout') %}
				<li class="nav-item dropdown no-arrow">
					<a class="nav-link dropdown-toggle" href="#" id="userDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
						<i class="fas fa-user-circle fa-fw"></i>
					</a>
					<div class="dropdown-menu dropdown-menu-right user-dropdown" aria-labelledby="userDropdown">
						<a class="dropdown-item" href="{{ url('PServerCore') }}"><i class="fas fa-cog fa-fw"></i> {{ translate('Page') }}</a>
						<div class="dropdown-divider"></div>
						<a class="dropdown-item" href="{{ url('small-user-auth',{'action':'logout'}) }}"><i class="fas fa-sign-out-alt fa-fw"></i> {{ translate('Logout') }}</a>
					</div>
					<!-- /.dropdown-user -->
				</li>
			{% endif %}
		</ul>
	</nav>
	<div id="wrapper">
		<!-- Sidebar -->
		<ul class="sidebar navbar-nav">
			{{ navigationWidgetPServerCore().menu('Laminas\\Navigation\\PServerAdmin').setPartial('p-server-admin/navigation')|raw }}
		</ul>
		<div id="content-wrapper">

			<div class="container-fluid">
				<!-- Charts Start -->
				<div class="page-head">
					<h4 class="my-2 bd-title">{{ navigationWidgetPServerCore().findAllBy('active', true)|last.getLabel }}{{ block('title') }}</h4>
					<small class="pull-right">{{ navigationWidgetPServerCore().breadcrumbs().setMinDepth(0).setRenderInvisible(true).setSeparator(' / ')|raw }}</small>
					<span class="clearfix"></span>
				</div>
				<hr>
				{% block content %}{{ content|raw }}{% endblock content %}
			</div>
			<footer class="sticky-footer">
				<div class="container my-auto">
					<div class="copyright text-center my-auto">
						<span style="font-size: 15px;">
							&copy; {{ "NOW"|date('Y') }} <b>PServerCMS</b> |
							<a href="https://github.com/kokspflanze/PServerCMS" target="_blank">
								<i class="fab fa-github black"></i>
							</a>
						</span>
					</div>
				</div>
			</footer>
		</div>
		<!-- Scroll to Top Button-->
		<a class="scroll-to-top rounded" href="#page-top" style="display: inline;">
			<i class="fas fa-angle-up"></i>
		</a>
	</div>


    {{ headScript()
		.prependFile(
			'https://code.jquery.com/jquery-3.5.1.min.js',
			'text/javascript',
			{'crossorigin' : 'anonymous'}
		)
		.appendFile(
			'https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js',
			'text/javascript',
			{
				'crossorigin' : 'anonymous',
				'integrity' : 'sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49',
			}
		)
		.appendFile(
			'https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js',
			'text/javascript',
			{
				'crossorigin' : 'anonymous',
				'integrity' : 'sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy',
			}
		)
		.appendFile(
			'https://code.jquery.com/ui/1.12.1/jquery-ui.min.js',
			'text/javascript',
			{'crossorigin' : 'anonymous'}
		)
		.appendFile(
			basePath('/assets/admin/js/sb-admin-2.min.js')
		)
		.appendFile(
			basePath('/js/function.js')
		)|raw
    }}
	<!-- Scripts -->
	{% block script %}{% endblock script %}
	<script>
		jQuery('.datepicker').datepicker({
			'dateFormat' : 'yy-mm-dd'
		});
	</script>
</body>
</html>
